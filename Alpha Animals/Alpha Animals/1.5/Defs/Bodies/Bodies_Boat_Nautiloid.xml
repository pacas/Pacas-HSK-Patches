						</li>

					</parts>
				</li>
				<li>
					<def>AA_Mast</def>
					<customLabel>first mast</customLabel>
					<coverage>0.25</coverage>
				</li>
				<li>
					<def>AA_Mast</def>
					<customLabel>second mast</customLabel>
					<coverage>0.15</coverage>
				</li>
				<li>
					<def>AA_Mast</def>
					<customLabel>third mast</customLabel>
					<coverage>0.1</coverage>
				</li>
				<li>
					<def>AA_NautilusShell</def>
					<coverage>0.25</coverage>
				</li>
			</parts>
		</corePart>
