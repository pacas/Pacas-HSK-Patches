<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<!-- ======================== TerramorphWorm ======================== -->

	<BodyDef>
		<defName>AA_WormWithArmorAndTusks</defName>
		<label>worm with armor and tusks</label>
		<corePart>
			<def>SnakeBody</def>
			<coverage>0.10</coverage>
			<height>Middle</height>
			<depth>Outside</depth>
			
			<parts>
				<li>
					<def>AA_FirstWormSegment</def>
					<coverage>0.20</coverage>
					<height>Middle</height>
					<depth>Outside</depth>
					<parts>
						<li>
							<def>Heart</def>
							<coverage>0.1</coverage>
							<depth>Inside</depth>
						</li>
					</parts>
				</li>
				<li>
					<def>AA_SecondWormSegment</def>
					<coverage>0.20</coverage>
					<height>Middle</height>
					<depth>Outside</depth>
					<parts>
						<li>
							<def>Lung</def>
												<customLabel>left lung</customLabel>

							<coverage>0.05</coverage>
							<depth>Inside</depth>
						</li>
						<li>
							<def>Lung</def>
												<customLabel>right lung</customLabel>

							<coverage>0.05</coverage>
							<depth>Inside</depth>
						</li>
					</parts>
				</li>
				<li>
					<def>AA_ThirdWormSegment</def>
					<coverage>0.20</coverage>
					<height>Middle</height>
					<depth>Outside</depth>
					<parts>
						<li>
							<def>Stomach</def>
							<coverage>0.05</coverage>
							<depth>Inside</depth>
						</li>
						<li>
							<def>Liver</def>
							<coverage>0.05</coverage>
							<depth>Inside</depth>
						</li>
					</parts>
				</li>
				<li>
					<def>AA_FourthWormSegment</def>
					<coverage>0.20</coverage>
					<height>Middle</height>
					<depth>Outside</depth>
					<parts>
						<li>
							<def>Kidney</def>
												<customLabel>left kidney</customLabel>

							<coverage>0.05</coverage>
							<depth>Inside</depth>
						</li>
						<li>
							<def>Kidney</def>
												<customLabel>right kidney</customLabel>

							<coverage>0.05</coverage>
							<depth>Inside</depth>
						</li>
					</parts>
				</li>
				<li>
					<def>SnakeHead</def>
					<coverage>0.1</coverage>
					<parts>
						<li>
						<def>Skull</def>
						<coverage>0.15</coverage>
						<depth>Inside</depth>
						<parts>
							<li>
								<def>Brain</def>
								<coverage>0.5</coverage>
								<depth>Inside</depth>
							</li>
						</parts>
						</li>
						<li>
							<def>Eye</def>
																			<customLabel>left eye</customLabel>

							<coverage>0.15</coverage>
						</li>
						<li>
							<def>Eye</def>
																										<customLabel>right eye</customLabel>

							<coverage>0.15</coverage>
						</li>
						<li>
							<def>Nose</def>
							<coverage>0.1</coverage>
						</li>
						<li>
							<def>Jaw</def>
							<coverage>0.1</coverage>
							<parts>
								<li>
									<def>Tusk</def>
																												<customLabel>left tusk</customLabel>

									<coverage>0.1</coverage>
									<depth>Outside</depth>
									<groups>
										<li>TuskAttackTool</li>
									</groups>
								</li>
								<li>
									<def>Tusk</def>
																												<customLabel>right tusk</customLabel>

									<coverage>0.1</coverage>
									<depth>Outside</depth>
									<groups>
										<li>TuskAttackTool</li>
									</groups>
								</li>
							</parts>
						</li>
					</parts>
				</li>
			</parts>
		</corePart>
	</BodyDef>

</Defs>