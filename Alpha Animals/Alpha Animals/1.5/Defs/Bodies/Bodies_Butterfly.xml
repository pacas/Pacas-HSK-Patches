			<height>Middle</height>
			<depth>Outside</depth>
			<parts>

				<li>
					<def>AA_Thorax</def>
					<coverage>0.10</coverage>
				</li>
				<li>
					<def>Stomach</def>
					<coverage>0.05</coverage>
					<depth>Inside</depth>
				</li>
				<li>
					<def>InsectHeart</def>
					<coverage>0.03</coverage>
					<depth>Inside</depth>
				</li>
				<li>
					<def>Pronotum</def>
					<coverage>0.08</coverage>
					<height>Top</height>
					<parts>
						<li>
							<def>InsectHead</def>
							<coverage>0.65</coverage>

							<parts>
								<li>
									<def>Brain</def>
									<coverage>0.2</coverage>
									<depth>Inside</depth>
								</li>
								<li>
									<def>AA_EcholocationOrgan</def>

									<coverage>0.1</coverage>
								</li>
								<li>
									<def>Antenna</def>
