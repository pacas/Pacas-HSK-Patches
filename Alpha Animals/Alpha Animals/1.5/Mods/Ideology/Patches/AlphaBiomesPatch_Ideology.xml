<?xml version="1.0" encoding="utf-8" ?>
<Patch>
    <Operation Class="PatchOperationFindMod">
        <mods>
            <li>Alpha Biomes</li>
        </mods>
        <match Class="PatchOperationSequence">
            <operations>
                <li Class="PatchOperationAdd">
                    <xpath>/Defs</xpath>
                    <value>
                        <TerrainDef ParentName="AA_OcularStone">
                            <defName>AA_Tile_OcularQuartz</defName>
                            <label>ocular red quartz floor</label>
                            <color>(120,50,50)</color>
                            <costList>
                                <GU_BlocksRoseQuartz>20</GU_BlocksRoseQuartz>
                            </costList>
                        </TerrainDef>
                        <TerrainDef ParentName="AA_OcularStone">
                            <defName>AB_Tile_OcularMudstone</defName>
                            <label>ocular mudstone floor</label>
                            <color>(49,31,15)</color>
                            <costList>
                                <AB_BlocksMudstone>20</AB_BlocksMudstone>
                            </costList>
                        </TerrainDef>
                        <TerrainDef ParentName="AA_OcularStone">
                            <defName>AB_Tile_OcularCragstone</defName>
                            <label>ocular cragstone floor</label>
                            <color>(36,35,40)</color>
                            <costList>
                                <AB_BlocksCragstone>20</AB_BlocksCragstone>
                            </costList>
                        </TerrainDef>
                        <TerrainDef ParentName="AA_OcularStone">
                            <defName>AB_Tile_OcularObsidian</defName>
                            <label>ocular obsidian floor</label>
                            <color>(50,50,50)</color>
                            <costList>
                                <AB_BlocksObsidian>20</AB_BlocksObsidian>
                            </costList>
                        </TerrainDef>
                    </value>
                </li>
                <li Class="PatchOperationReplace">
                    <xpath>/Defs/ThingDef[defName="AA_Dryad_Ocular"]/comps</xpath>
                    <value>
                        <comps>
                            <li Class="CompProperties_Spawner">
                                <thingToSpawn>GU_RedWood</thingToSpawn>
                                <spawnCount>40</spawnCount>
                                <spawnIntervalRange>120000</spawnIntervalRange>
                                <writeTimeLeftToSpawn>true</writeTimeLeftToSpawn>
                                <showMessageIfOwned>true</showMessageIfOwned>
                            </li>
                        </comps>
                    </value>
                </li>
            </operations>
        </match>
    </Operation>
</Patch>
