<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<!-- Ranged spinneret -->

	<RecipeDef ParentName="VEF_AdvancedSurgeryInstallImplantBase">
		<defName>AA_InstallRangedSpinneretImplant</defName>
		<label>install ranged spinneret implant</label>
		<description>Install a ranged spinneret. This implant will allow this creature to use a ranged stunning web attack.\n\nNote that installing this on a creature that already has a ranged spinneret will have no effect.</description>
		<descriptionHyperlinks>
			<ThingDef>AA_RangedSpinneretImplant</ThingDef>
			<HediffDef>AA_RangedSpinneretImplant</HediffDef>
		</descriptionHyperlinks>
		<jobString>Installing ranged spinneret implant.</jobString>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>AA_RangedSpinneretImplant</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>AA_RangedSpinneretImplant</li>
			</thingDefs>
		</fixedIngredientFilter>
		<appliedOnFixedBodyParts>
			<li>Body</li>								
			<li>SnakeBody</li>
			<li>AA_JellyfishMainBody</li>
			<li>AA_Protoplasm</li>
			<li>AA_Thorax</li>
			<li>AA_SentientPlantBody</li>
			<li>AA_SlugBody</li>
		</appliedOnFixedBodyParts>
		<addsHediff>AA_RangedSpinneretImplant</addsHediff>
	</RecipeDef>

	<RecipeDef ParentName="SurgeryRemoveImplantBase">
		<defName>AA_RemoveRangedSpinneretImplant</defName>
		<label>remove ranged spinneret implant</label>
		<description>Remove ranged spinneret implant.</description>
		<descriptionHyperlinks>
			<ThingDef>AA_RangedSpinneretImplant</ThingDef>
			<HediffDef>AA_RangedSpinneretImplant</HediffDef>
		</descriptionHyperlinks>
		<jobString>Removing ranged spinneret implant.</jobString>
		<removesHediff>AA_RangedSpinneretImplant</removesHediff>
	</RecipeDef>

	<!-- Venomous Spitter -->

	<RecipeDef ParentName="VEF_AdvancedSurgeryInstallImplantBase">
		<defName>AA_InstallVenomousSpitterImplant</defName>
		<label>install venomous spitter implant</label>
		<description>Install a venomous spitter. This implant will allow this creature to use a ranged venomous spit attack.\n\nNote that installing this on a creature that already has a venomous spitter will have no effect. This implant can only be used by animals that have a mouth (and not, for example, a beak or proboscis).</description>
		<descriptionHyperlinks>
			<ThingDef>AA_VenomousSpitterImplant</ThingDef>
			<HediffDef>AA_VenomousSpitterImplant</HediffDef>
		</descriptionHyperlinks>
		<jobString>Installing venomous spitter implant.</jobString>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>AA_VenomousSpitterImplant</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>AA_VenomousSpitterImplant</li>
			</thingDefs>
		</fixedIngredientFilter>
		<appliedOnFixedBodyParts>
			<li>AnimalJaw</li>								
			<li>AA_InsectMouth</li>
			<li>AA_CrocodileJaw</li>
			<li>AA_SentientPlantMouth</li>
		</appliedOnFixedBodyParts>
		<addsHediff>AA_VenomousSpitterImplant</addsHediff>
	</RecipeDef>

	<RecipeDef ParentName="SurgeryRemoveImplantBase">
		<defName>AA_RemoveVenomousSpitterImplant</defName>
		<label>remove venomous spitter implant</label>
		<description>Remove venomous spitter implant.</description>
