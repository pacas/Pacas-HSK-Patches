<Defs>
    <!--=============== Tentacles ====================-->
    <HediffDef ParentName="addedPartAnimal">
        <defName>AA_ProstheticTentacle</defName>
        <label>simple prosthetic tentacle</label>
        <description>A simple prosthetic tentacle.</description>
        <defaultLabelColor>(233,129,69)</defaultLabelColor>
        <comps>
            <li Class="HediffCompProperties_VerbGiver">
                <tools>
                    <li>
                        <label>prosthetic tentacle</label>
                        <capacities>
                            <li>Blunt</li>
                        </capacities>
                        <power>8</power>
                        <cooldownTime>1.8</cooldownTime>
                        <alwaysTreatAsWeapon>false</alwaysTreatAsWeapon>
                    </li>
                </tools>
            </li>
        </comps>
        <spawnThingOnRemoved>AA_ProstheticTentacle</spawnThingOnRemoved>
        <addedPartProps>
            <partEfficiency>0.85</partEfficiency>
            <betterThanNatural>false</betterThanNatural>
            <solid>true</solid>
        </addedPartProps>
        <stages>
            <li>
                <statOffsets>
                    <ArmorRating_Sharp>0.01</ArmorRating_Sharp>
                    <ArmorRating_Blunt>0.01</ArmorRating_Blunt>
                    <ArmorRating_Heat>0.01</ArmorRating_Heat>
                </statOffsets>
            </li>
        </stages>
    </HediffDef>
    <HediffDef ParentName="addedPartAnimal">
        <defName>AA_BionicTentacle</defName>
        <label>bionic tentacle</label>
        <description>A bionic tentacle.</description>
