                        <alwaysTreatAsWeapon>false</alwaysTreatAsWeapon>
                    </li>
                </tools>
            </li>
        </comps>
        <spawnThingOnRemoved>AA_BionicTentacle</spawnThingOnRemoved>
        <addedPartProps>
            <solid>true</solid>
            <partEfficiency>1.25</partEfficiency>
        </addedPartProps>
        <stages>
            <li>
                <capMods>
                    <li>
                        <capacity>Manipulation</capacity>
