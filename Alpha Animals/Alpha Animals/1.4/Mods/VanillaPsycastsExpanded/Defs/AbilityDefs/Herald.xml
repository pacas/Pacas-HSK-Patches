<?xml version="1.0" encoding="UTF-8"?>
<Defs>
    <VFECore.Abilities.AbilityDef ParentName="VPE_PsycastBase">
        <defName>AAVPE_VenomSpit</defName>
        <label>venom spit</label>
        <description>Generates a small blob of caustic liquid by linking a microscopic skipgate to an underground reservoir of acid. This blob is then psychically charged and accelerated in the desired direction.</description>
        <iconPath>UI/Abilities/Herald/AAVPE_Spit</iconPath>
        <abilityClass>VFECore.Abilities.Ability_ShootProjectile</abilityClass>
        <castTime>60</castTime>
        <range>16.9</range>
        <targetMode>Pawn</targetMode>
        <targetingParameters>
            <canTargetPawns>true</canTargetPawns>
            <canTargetBuildings>false</canTargetBuildings>
            <canTargetSelf>false</canTargetSelf>
        </targetingParameters>
        <castSound>AA_PoisonBolt</castSound>
        <modExtensions>
            <li Class="VanillaPsycastsExpanded.AbilityExtension_Psycast">
                <path>AAVPE_HeraldOfTheBlackHive</path>
                <level>1</level>
                <order>1</order>
                <psyfocusCost>0.01</psyfocusCost>
                <entropyGain>2</entropyGain>
            </li>
            <li Class="VFECore.Abilities.AbilityExtension_Projectile">
                <projectile>AA_PoisonBolt</projectile>
            </li>
        </modExtensions>
    </VFECore.Abilities.AbilityDef>
    
    <VFECore.Abilities.AbilityDef ParentName="VPE_PsycastBase">
        <defName>AAVPE_SummonBlackScarab</defName>
        <label>summon black scarab</label>
        <description>Psychically skips a black scarab to help defend the colony. The black scarab will stay around for a day before vanishing, and will be hostile to any enemy from outside the colony.</description>
        <iconPath>UI/Abilities/Herald/AAVPE_SummonBlackScarab</iconPath>
        <abilityClass>AlphaBehavioursAndEvents.Ability_Summon</abilityClass>
        <requireLineOfSight>false</requireLineOfSight>
        <range>20</range>
        <distanceToTarget>2.9</distanceToTarget>
        <targetMode>Location</targetMode>
        <castTime>360</castTime>
        <needsTicking>true</needsTicking>
        <showUndrafted>true</showUndrafted>
        <targetingParameters>
            <canTargetLocations>true</canTargetLocations>
        </targetingParameters>
        <modExtensions>
            <li Class="VanillaPsycastsExpanded.AbilityExtension_Psycast">
                <path>AAVPE_HeraldOfTheBlackHive</path>
                <level>2</level>
                <order>1</order>
                <psyfocusCost>0.1</psyfocusCost>
                <entropyGain>10</entropyGain>
                <prerequisites>
                    <li>AAVPE_VenomSpit</li>
                </prerequisites>
            </li>
            <li Class="VFECore.Abilities.AbilityExtension_FleckOnTarget">
                <fleckDefs>
                    <li>PsycastSkipInnerExit</li>
                    <li>PsycastSkipOuterRingExit</li>
                </fleckDefs>
                <scale>0.42</scale>
                <sound>Psycast_Skip_Exit</sound>
                <preCastTicks>10</preCastTicks>
            </li>
            <li Class="VFECore.Abilities.AbilityExtension_EffecterOnTarget">
                <effecterDef>Skip_Exit</effecterDef>
                <maintainForTicks>60</maintainForTicks>
                <scale>0.42</scale>
            </li>
            <li Class="AlphaBehavioursAndEvents.Ability_Summon_Extension">
                <pawnToSpawn>AA_BlackScarab_Temporary</pawnToSpawn>
                <enrage>true</enrage>
                <playerFaction>true</playerFaction>
                <numberCreated>1</numberCreated>
            </li>
        </modExtensions>
    </VFECore.Abilities.AbilityDef>
    
