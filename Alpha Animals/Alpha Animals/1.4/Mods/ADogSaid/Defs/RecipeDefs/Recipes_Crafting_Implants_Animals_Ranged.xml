<Defs>
	<!-- Ranged spinneret -->

	<RecipeDef>
		<defName>AA_MakeRangedSpinneretImplant</defName>
		<label>make a ranged spinneret implant</label>
		<description>Make a ranged spinneret implant from a Feralisk, Dunealisk, Blizzarisk, Cinderlisk, Junglelisk or Animalisk corpse. This implant will allow this creature to use a ranged stunning web attack.\n\nNote that installing this on a creature that already has a ranged spinneret will have no effect.</description>
		<jobString>Making ranged spinneret implant</jobString>
		<workAmount>15000</workAmount>
		<recipeUsers>
			<li>VEF_AdvancedAnimalImplantsTable</li>
		</recipeUsers>
		<workSpeedStat>SmithingSpeed</workSpeedStat>
		<workSkill>Crafting</workSkill>
		<effectWorking>Smith</effectWorking>
		<soundWorking>Recipe_Smith</soundWorking>
		<skillRequirements>
			<Crafting>10</Crafting>
		</skillRequirements>
		<unfinishedThingDef>UnfinishedAnimalPart</unfinishedThingDef>
		<researchPrerequisite>VEF_AdvancedAnimalImplants</researchPrerequisite>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>Corpse_AA_Feralisk</li>
						<li>Corpse_AA_Blizzarisk</li>
						<li>Corpse_AA_Dunealisk</li>
						<li>Corpse_AA_Cinderlisk</li>
						<li>Corpse_AA_Junglelisk</li>
						<li>Corpse_AA_Animalisk</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Biomatter</li>
					</thingDefs>
				</filter>
				<count>30</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>ComponentAdvanced</li>
					</thingDefs>
				</filter>
				<count>2</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>SyntheticFibers</li>
					</thingDefs>
				</filter>
				<count>15</count>
			</li>
