    <RecipeDef Name="VEF_AdvancedSurgeryInstallImplantBase" ParentName="SurgeryFlesh" Abstract="True">
        <workerClass>Recipe_InstallImplant</workerClass>
        <workAmount>2500</workAmount>
        <skillRequirements>
            <Medicine>8</Medicine>
        </skillRequirements>
        <ingredients>
            <li>
                <filter>
                    <categories>
                        <li>Medicine</li>
                    </categories>
                </filter>
                <count>2</count>
            </li>
        </ingredients>
