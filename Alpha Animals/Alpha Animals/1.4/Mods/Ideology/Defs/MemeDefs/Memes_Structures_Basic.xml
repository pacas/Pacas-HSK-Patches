                <li>memeAdjective->alien</li>
                <li>memeAdjective->extraterrestrial</li>
                <li>memeAdjective->strange</li>
                <li>memeAdjective->peculiar</li>
                <li>memeAdjective->exotic</li>
                <li>memeLeaderNoun->high alienist</li>
                <li>memeLeaderAdjective->foreign</li>
                <li>memeMoralist->aspirant</li>
