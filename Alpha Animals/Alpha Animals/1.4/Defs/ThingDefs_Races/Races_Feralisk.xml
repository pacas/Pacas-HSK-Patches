<?xml version="1.0" encoding="utf-8" ?>
<Defs>
    <ThingDef ParentName="SK_AA_Spiders">
        <defName>AA_Feralisk</defName>
        <label>feralisk</label>
        <description>Someone once thought that creating supersized spiders for synthread-web production was a good idea. Their home planet is now reduced to a Marble World; a world completely purged by atomic fire.\n\nFeralisks attack by throwing spider webs at their prey. These huge webs stun anyone they touch, allowing the Feralisk to close in and finish the kill with their fearsome venom laced fangs.</description>
        <statBases>
            <MoveSpeed>5.5</MoveSpeed>
            <ComfyTemperatureMin>-12</ComfyTemperatureMin>
            <ComfyTemperatureMax>60</ComfyTemperatureMax>
        </statBases>
        <tools>
            <li>
                <label>left first leg</label>
                <capacities>
                    <li>Scratch</li>
                </capacities>
                <power>12</power>
                <cooldownTime>1.5</cooldownTime>
                <linkedBodyPartsGroup>LegAttackTool</linkedBodyPartsGroup>
            </li>
            <li>
                <label>right first leg</label>
                <capacities>
                    <li>Scratch</li>
                </capacities>
                <power>12</power>
                <cooldownTime>1.5</cooldownTime>
                <linkedBodyPartsGroup>LegAttackTool</linkedBodyPartsGroup>
            </li>
            <li>
                <capacities>
                    <li>AA_ToxicSting</li>
                </capacities>
                <power>15</power>
                <cooldownTime>1.65</cooldownTime>
                <linkedBodyPartsGroup>Mouth</linkedBodyPartsGroup>
                <surpriseAttack>
                    <extraMeleeDamages>
                        <li>
                            <def>Stun</def>
                            <amount>20</amount>
                        </li>
                    </extraMeleeDamages>
                </surpriseAttack>
            </li>
            <li>
                <label>head</label>
                <capacities>
                    <li>Blunt</li>
                </capacities>
                <power>10</power>
                <cooldownTime>1.65</cooldownTime>
                <linkedBodyPartsGroup>HeadAttackTool</linkedBodyPartsGroup>
                <chanceFactor>0.2</chanceFactor>
            </li>
        </tools>
        <verbs>
            <li>
                <verbClass>Verb_Shoot</verbClass>
                <accuracyTouch>1</accuracyTouch>
                <accuracyShort>1</accuracyShort>
                <accuracyMedium>1</accuracyMedium>
                <accuracyLong>1</accuracyLong>
                <hasStandardCommand>true</hasStandardCommand>
                <defaultProjectile>AA_Web</defaultProjectile>
                <warmupTime>3</warmupTime>
                <burstShotCount>3</burstShotCount>
                <minRange>1</minRange>
                <range>20</range>
                <soundCast>AA_Throw</soundCast>
                <muzzleFlashScale>0</muzzleFlashScale>
                <label>web</label>
                <commonality>.8</commonality>
            </li>
        </verbs>
        <comps>
            <li Class="AnimalBehaviours.CompProperties_AnimalProduct">
                <resourceDef>Synthread</resourceDef>
                <gatheringIntervalDays>22</gatheringIntervalDays>
                <resourceAmount>33</resourceAmount>
                <customResourceString>SynthreadDesc</customResourceString>
            </li>
            <li Class="CompProperties_EggLayer">
                <eggUnfertilizedDef>AA_FeraliskEggUnfertilized</eggUnfertilizedDef>
                <eggFertilizedDef>AA_FeraliskEggFertilized</eggFertilizedDef>
                <eggFertilizationCountMax>1</eggFertilizationCountMax>
                <eggLayIntervalDays>15</eggLayIntervalDays>
                <eggCountRange>
                    <min>1</min>
                    <max>3</max>
                </eggCountRange>
            </li>
            <li Class="MVCF.Comps.CompProperties_RandomVerbs">
				<meleeChance>0.2</meleeChance>
			</li>
        </comps>
        <race>
            <wildBiomes>
                <TemperateSwamp>0.15</TemperateSwamp>
                <TropicalSwamp>0.15</TropicalSwamp>
                <TemperateForest>0.07</TemperateForest>
                <TropicalRainforest>0.15</TropicalRainforest>
            </wildBiomes>
            <canBePredatorPrey>false</canBePredatorPrey>
            <predator>true</predator>
            <body>AA_Arachnid</body>
            <herdAnimal>true</herdAnimal>
            <baseBodySize>1.00</baseBodySize>
            <baseHungerRate>0.3</baseHungerRate>
            <baseHealthScale>1.2</baseHealthScale>
            <foodType>OmnivoreAnimal, AnimalProduct</foodType>
            <wildness>0.985</wildness>
            <trainability>Advanced</trainability>
            <litterSizeCurve>
                <points>
                    <li>(0.5, 0)</li>
                    <li>(1, 1)</li>
                    <li>(2, 1.7)</li>
                    <li>(3, 0.7)</li>
                    <li>(3.5, 0)</li>
                </points>
            </litterSizeCurve>
            <lifeExpectancy>12</lifeExpectancy>
            <lifeStageAges>
                <li>
                    <def>AnimalBaby</def>
                    <minAge>0</minAge>
                </li>
                <li>
                    <def>AnimalJuvenile</def>
                    <minAge>0.24</minAge>
                </li>
                <li>
                    <def>AnimalAdult</def>
                    <minAge>1.20</minAge>
                    <soundWounded>Pawn_Feralisk_Wounded</soundWounded>
                    <soundDeath>Pawn_Feralisk_Death</soundDeath>
                    <soundCall>Pawn_Feralisk_Call</soundCall>
                    <soundAngry>Pawn_Feralisk_Angry</soundAngry>
                </li>
            </lifeStageAges>
            <soundMeleeHitPawn>Pawn_Melee_BigBash_HitPawn</soundMeleeHitPawn>
            <soundMeleeHitBuilding>Pawn_Melee_BigBash_HitBuilding</soundMeleeHitBuilding>
            <soundMeleeMiss>Pawn_Melee_BigBash_Miss</soundMeleeMiss>
        </race>
        <tradeTags>
            <li>AnimalExotic</li>
        </tradeTags>
        <modExtensions>
            <li Class="AnimalBehaviours.AnimalStatExtension">
                <statToAdd>
                    <li>AA_SecondaryRole</li>
                    <li>AA_PrimaryRole</li>
                    <li>AA_RangedAttacks</li>
                </statToAdd>
                <statValues>
                    <li>AA_Farm</li>
                    <li>AA_ParalizingRanged</li>
                    <li>AA_Yes</li>
                </statValues>
                <statDescriptions>
                    <li>AA_FarmWoolDesc</li>
                    <li>AA_ParalyzingRangedDesc</li>
                    <li>AA_Ranged</li>
                </statDescriptions>
            </li>
        </modExtensions>
    </ThingDef>
