        <graphicData>
            <color>(107,123,156)</color>
        </graphicData>
        <statBases>
            <MarketValue>3</MarketValue>
            <StuffPower_Armor_Sharp>0.84</StuffPower_Armor_Sharp>
            <StuffPower_Armor_Blunt>0.75</StuffPower_Armor_Blunt>
