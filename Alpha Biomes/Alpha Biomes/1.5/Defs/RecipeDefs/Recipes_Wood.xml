<?xml version="1.0" encoding="utf-8" ?>
<Defs>
    <RecipeDef>
		<defName>Make_AB_Crystal_WoodPlanks_Hand</defName>
		<label>saw crystalline planks</label>
		<description>Saws crystalline logs into planks. Quite slow. Produces 15.</description>
		<jobString>Sawing crystalline planks.</jobString>
		<workAmount>1200</workAmount>
		<workSpeedStat>SmithingSpeed</workSpeedStat>
		<effectWorking>MakeWoodPlanks_Hand</effectWorking>
		<soundWorking>Sawing_Wood_Sound</soundWorking>
        <recipeUsers>
			<li>TableSawmillHand</li>
		</recipeUsers>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>AB_CrystalWood</li>
					</thingDefs>
				</filter>
				<count>10</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>AB_CrystalWood</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<AB_Crystal_WoodPlank>15</AB_Crystal_WoodPlank>
		</products>
		<skillRequirements>
			<Crafting>3</Crafting>
		</skillRequirements>
		<workSkill>Crafting</workSkill>
		<workSkillLearnFactor>0.8</workSkillLearnFactor>
		<researchPrerequisite>Craft_0</researchPrerequisite>
	</RecipeDef>

