<?xml version="1.0" encoding="utf-8" ?>
<Defs>

    <!--=====================================================================-->
    <!--=============================== Fish ================================-->
    <!--=====================================================================-->


    <ThingDef Name="AB_RawFishBase" Abstract="True" ParentName="OrganicProductBase">
        <label>Base raw fish properties</label>
        <comps>
            <li Class="CompProperties_Rottable">
                <daysToRotStart>2</daysToRotStart>
                <rotDestroys>True</rotDestroys>
            </li>
        </comps>
        <statBases>
            <Nutrition>0.05</Nutrition>
            <Flammability>0.5</Flammability>
            <FoodPoisonChanceFixedHuman>0.02</FoodPoisonChanceFixedHuman>
        </statBases>
        <ingestible>
            <foodType>Meat</foodType>
            <preferability>DesperateOnly</preferability>
            <ingestEffect>EatMeat</ingestEffect>
            <ingestSound>RawMeat_Eat</ingestSound>
        </ingestible>
        <thingCategories>
            <li>FoodFreshFish</li>
        </thingCategories>
    </ThingDef>
	
    <!-- Abyssal Eel -->

    <ThingDef ParentName="AB_RawFishBase">
        <defName>VCEF_AbyssalEel</defName>
        <label>abyssal eel</label>
        <description>Raw abyssal eel. This elongated fish has adapted to life at very low temperatures. Abyssal eel have an incredibly slow metabolism, only needing to feed every few weeks.</description>
        <graphicData>
            <texPath>Things/Item/Resource/Fish/VCEF_AbyssalEel</texPath>
            <graphicClass>Graphic_StackCount</graphicClass>
        </graphicData>
        <statBases>
            <Mass>0.03</Mass>
            <Nutrition>0.05</Nutrition>
            <MarketValue>1.1</MarketValue>
        </statBases>
        <ingestible>
            <specialThoughtDirect>AteRawFood</specialThoughtDirect>
        </ingestible>
        <uiIconForStackCount>1</uiIconForStackCount>
    </ThingDef>
	
    <!-- Blood Leech -->

    <ThingDef ParentName="AB_RawFishBase">
        <defName>VCEF_BloodLeech</defName>
        <label>blood leech</label>
        <description>Raw blood leech. This bright red lamprey-like creature feeds almost exclusively on the blood of larger aquatic organisms, although it is not adverse to taking a bite from some terrestrial ones that come near the water. It tastes metallic and unappealing.</description>
        <graphicData>
            <texPath>Things/Item/Resource/Fish/VCEF_BloodLeech</texPath>
            <graphicClass>Graphic_StackCount</graphicClass>
        </graphicData>
        <statBases>
            <Mass>0.01</Mass>
            <Nutrition>0.01</Nutrition>
            <MarketValue>0.22</MarketValue>
        </statBases>
        <ingestible>
            <specialThoughtDirect>AteRawFood</specialThoughtDirect>
        </ingestible>
        <uiIconForStackCount>1</uiIconForStackCount>
    </ThingDef>
	
    <!-- Boreal Sardine -->

    <ThingDef ParentName="AB_RawFishBase">
        <defName>VCEF_BorealSardine</defName>
        <label>boreal sardine</label>
        <description>Raw boreal sardine. This small, oily forage fish in the herring family is well adapted to very cold climates.</description>
        <graphicData>
            <texPath>Things/Item/Resource/Fish/VCEF_BorealSardine</texPath>
            <graphicClass>Graphic_StackCount</graphicClass>
        </graphicData>
        <statBases>
