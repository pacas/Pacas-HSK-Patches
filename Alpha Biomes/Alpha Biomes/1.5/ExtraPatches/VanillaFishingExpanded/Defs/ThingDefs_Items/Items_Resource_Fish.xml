    <!--  Ocular Fish -->

    <ThingDef ParentName="AB_RawFishBase">
        <defName>VCEF_OcularFish</defName>
        <label>ocular fish</label>
        <description>Raw ocular fish. A completely alien breed of fish. Its single, huge yellowed eye seems to be looking in all directions simultaneously.</description>
        <graphicData>
            <texPath>Things/Item/Resource/Fish/VCEF_OcularFish</texPath>
