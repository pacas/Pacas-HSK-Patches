    <!--  Shadow Fry -->

    <ThingDef ParentName="AB_RawFishBase">
        <defName>VCEF_ShadowFry</defName>
        <label>shadow fry</label>
        <description>Raw shadow fry. Small river fish that move in large schools around fishing banks and near riverbanks, tending to hide from sunlight in darkened crevices.</description>
        <graphicData>
