<?xml version="1.0" encoding="utf-8" ?>
<Patch>
    <Operation Class="PatchOperationFindMod">
        <mods>
            <li>Alpha Genes</li>
        </mods>
        <match Class="PatchOperationSequence">
            <operations>
                <li Class="PatchOperationAdd">
                    <xpath>Defs/XenotypeDef[defName="RevianXenotype"]/genes</xpath>
                    <value>
                        <li>AG_Female</li>
                    </value>
                </li>
            </operations>
        </match>
    </Operation>
    
    <Operation Class="PatchOperationFindMod">
        <mods>
            <li>Dominant Gene</li>
        </mods>
        <match Class="PatchOperationSequence">
            <operations>
                <li Class="PatchOperationAdd">
                    <xpath>Defs/XenotypeDef[defName="RevianXenotype"]/genes</xpath>
                    <value>
                        <li>DominantGene</li>
                    </value>
                </li>
            </operations>
        </match>
    </Operation>
</Patch>