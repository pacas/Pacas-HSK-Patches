<?xml version="1.0" encoding="utf-8"?><Defs>
 
  <!-- ===== Longbow Charge ==== -->
  
      	<!-- ===================== Sustainer ========================= -->

    <SoundDef>
    <defName>RHPawn_Stalker_Breathing</defName>
    <sustain>true</sustain>
    <context>MapOnly</context>
    <eventNames />
    <maxSimultaneous>1</maxSimultaneous>
    <priorityMode>PrioritizeNearest</priorityMode>  
    <subSounds>
      <li>
	  <onCamera>True</onCamera>  
        <tempoAffectedByGameSpeed>true</tempoAffectedByGameSpeed>
        <grains>
          <li Class="AudioGrain_Clip">
            <clipPath>DF2_Stalker/Stalker_Loop</clipPath>
          </li>
        </grains>      
        <pitchRange>
          <min>1</min>        
          <max>1</max>
        </pitchRange>
        <volumeRange>
          <min>50</min>        
          <max>50</max>
        </volumeRange>
      </li>
    </subSounds>
  </SoundDef>
  
  	    <!-- ===================== Stalker Call ========================= -->
  
      <SoundDef>
    <defName>RHPawn_Stalker_Call</defName>  
    <context>MapOnly</context>
    <eventNames />  
    <maxSimultaneous>1</maxSimultaneous>  
    <subSounds>
      <li>
        <grains>
          <li Class="AudioGrain_Clip">
            <clipPath>DF2_Stalker/Stalker_Call</clipPath>
          </li>
        </grains>      
        <pitchRange>
          <min>0.936413</min>        
          <max>1.042391</max>
        </pitchRange>      
        <paramMappings>
          <li>
            <inParam Class="SoundParamSource_CameraAltitude" />          
            <outParam Class="SoundParamTarget_PropertyLowPass">
              <filterProperty>Cutoff</filterProperty>
            </outParam>          
            <paramUpdateMode>Constant</paramUpdateMode>          
            <curve>
              <points>
                <li>
                  <loc>
                    <x>17.08957</x>                  
                    <y>21001.8</y>
                  </loc>
                </li>              
                <li>
                  <loc>
                    <x>24.77761</x>                  
                    <y>12701.95</y>
                  </loc>
                </li>              
                <li>
                  <loc>
                    <x>32.786</x>                  
                    <y>7168.717</y>
                  </loc>
                </li>              
              </points>
            </curve>
          </li>
        </paramMappings>      
        <filters>
          <li Class="SoundFilterLowPass">
            <cutoffFrequency>10000</cutoffFrequency>          
            <lowpassResonaceQ>1</lowpassResonaceQ>
          </li>
        </filters>      
        <sustainLoop>False</sustainLoop>
      </li>
    </subSounds>
  </SoundDef> 
  
