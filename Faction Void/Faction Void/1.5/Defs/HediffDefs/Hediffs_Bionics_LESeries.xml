    <!-- MOTL implant -->

    <HediffDef ParentName="HediffBodyPartBase_GlitterworldImplants">
        <defName>DF_MOTL_Implant</defName>
        <label>MOTL implant</label>
        <labelNoun>a MOTL implant</labelNoun>
        <description>An installed MOTL implant.</description>
        <descriptionHyperlinks>
            <ThingDef>DF_MOTL_Implant</ThingDef>
        </descriptionHyperlinks>
        <spawnThingOnRemoved>DF_MOTL_Implant</spawnThingOnRemoved>
        <stages>
            <li>
                <capMods>
                    <li>
                        <capacity>Moving</capacity>
                        <offset>0.05</offset>
                    </li>
                    <li>
                        <capacity>Consciousness</capacity>
                        <offset>0.05</offset>
                    </li>
                    <li>
                        <capacity>Manipulation</capacity>
                        <offset>0.05</offset>
                    </li>
                </capMods>
                <statOffsets>
                    <GlobalLearningFactor>0.05</GlobalLearningFactor>
                    <ArmorRating_Sharp>0.05</ArmorRating_Sharp>
                    <ArmorRating_Blunt>0.05</ArmorRating_Blunt>
                    <ArmorRating_Heat>0.05</ArmorRating_Heat>
                </statOffsets>
            </li>
        </stages>
        <tags>
            <li>DF_MOTL_Implant</li>
        </tags>
    </HediffDef>

    <ThingDef ParentName="RH_LE_ImplantBase">
        <defName>DF_MOTL_Implant</defName>
        <label>MOTL implant</label>
        <description>The March of the Living implant or MOTL for short. An implant invented by Dr. David Tolousse, a surviving Secronom scientist during the N-4 global pandemic. He invented this implant in mind in order to help survivors gain a greater edge in exploring the wastelands on the outskirts of the inner city, where the infected within were capable of outrunning an average adult human. Thanks to this implant, other innovations were developed that helped survivors who used them suffer less fatalities.</description>
        <descriptionHyperlinks>
            <RecipeDef>InstallDF_MOTL_Implant</RecipeDef>
        </descriptionHyperlinks>
        <graphicData>
            <texPath>Things/Item/Special/LE/MOTL_Implant</texPath>
            <graphicClass>Graphic_Single</graphicClass>
            <drawSize>1.0</drawSize>
            <color>(255, 255, 255)</color>
        </graphicData>
    </ThingDef>

    <RecipeDef ParentName="VOID_InstallImplantLE">
        <defName>InstallDF_MOTL_Implant</defName>
        <label>install MOTL implant</label>
        <uiIconThing>DF_MOTL_Implant</uiIconThing>
        <description>Install a MOTL implant.</description>
        <descriptionHyperlinks>
            <ThingDef>DF_MOTL_Implant</ThingDef>
        </descriptionHyperlinks>
        <jobString>Installing MOTL implant.</jobString>
        <ingredients>
            <li>
                <filter>
                    <thingDefs>
                        <li>DF_MOTL_Implant</li>
                    </thingDefs>
                </filter>
                <count>1</count>
            </li>
        </ingredients>
        <fixedIngredientFilter>
            <thingDefs>
                <li>DF_MOTL_Implant</li>
            </thingDefs>
        </fixedIngredientFilter>
        <appliedOnFixedBodyParts>
            <li>Torso</li>
        </appliedOnFixedBodyParts>
        <addsHediff>DF_MOTL_Implant</addsHediff>
        <!--<incompatibleWithHediffTags>
			<li>DF_TrinityImplant</li>
		</incompatibleWithHediffTags>-->
    </RecipeDef>

    <RecipeDef ParentName="VOID_RemoveImplantLE">
        <defName>RemoveDF_MOTL_Implant</defName>
        <label>remove MOTL implant</label>
        <description>Remove MOTL implant.</description>
