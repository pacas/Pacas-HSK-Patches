    <!-- ================================= BIG GAUN! ==================================== -->
    <!-- ================================= Longshot PP-10 ==================================== -->
    <ThingDef ParentName="SK_BaseGun">
        <defName>RNGun_Longshot_PP10AMR</defName>
        <label>longshot PP-10</label>
        <description>The Longshot PP-10, chambered in 14.5x115mm anti-materiel rifle cartridge. The rifle is built from Ironsight 33-F parts, vastly improved by the best surviving engineers and weapon technicians of Fairview. It's development along with the Wraith Cannon was sponsored by the most powerful factions of the inner city.</description>
        <graphicData>
            <texPath>Things/Item/Equipment/Weapons/Ranged/LongshotPP10</texPath>
            <graphicClass>Graphic_Single</graphicClass>
        </graphicData>
        <soundInteract>Interact_Rifle</soundInteract>
        <statBases>
            <MarketValue>3000</MarketValue>
            <Mass>4</Mass>
            <AccuracyTouch>0.59</AccuracyTouch>
            <AccuracyShort>0.70</AccuracyShort>
            <AccuracyMedium>0.94</AccuracyMedium>
            <AccuracyLong>0.85</AccuracyLong>
            <RangedWeapon_Cooldown>3.2</RangedWeapon_Cooldown>
        </statBases>
        <equippedAngleOffset>40</equippedAngleOffset>
        <verbs>
            <li>
                <verbClass>Verb_Shoot</verbClass>
                <hasStandardCommand>true</hasStandardCommand>
                <defaultProjectile>RHBullet_57CalFMJ_HMG</defaultProjectile>
                <warmupTime>3</warmupTime>
                <range>42.9</range>
                <burstShotCount>2</burstShotCount>
                <ticksBetweenBurstShots>45</ticksBetweenBurstShots>
                <soundCast>RNShot_DFGAUSSRifle</soundCast>
                <soundCastTail>GunTail_Heavy</soundCastTail>
                <muzzleFlashScale>9</muzzleFlashScale>
            </li>
        </verbs>
        <tools>
            <li>
                <label>stock</label>
                <capacities>
                    <li>Blunt</li>
                </capacities>
                <power>9</power>
                <cooldownTime>2</cooldownTime>
            </li>
            <li>
                <label>barrel</label>
                <capacities>
                    <li>Blunt</li>
                    <li>Poke</li>
                </capacities>
                <power>9</power>
                <cooldownTime>2</cooldownTime>
            </li>
            <li>
                <label>bayonet</label>
                <capacities>
                    <li>Stab</li>
                    <li>Cut</li>
                </capacities>
                <power>12</power>
