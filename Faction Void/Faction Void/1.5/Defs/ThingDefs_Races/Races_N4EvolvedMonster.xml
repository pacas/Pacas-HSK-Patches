					<extraMeleeDamages>
						<li>
							<def>Stun</def>
							<amount>30</amount>
						</li>
					</extraMeleeDamages>
				</surpriseAttack>
			</li>
			<li>
				<label>tongue</label>
				<capacities>
					<li>Cut</li>
